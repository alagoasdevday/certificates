<!DOCTYPE html>
<html>
<head>
  <title>Certificados | Alagoas Dev Day</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="container">
    <div class="row">
      <div class="col-md-12 logo">
        <%=image_tag "logo.jpg" %>
      </div>
      <div class="col-md-12">
        <%=flash_messages%>
        <%= yield %>
      </div>
    </div>
  </div>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
</body>
</html>
