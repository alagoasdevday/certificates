<h1>Digite seu email para pesquisar seus certificados</h1>
<%=form_tag search_events_path, method: :get, class: "form-horizontal" do%>
  <div class="form-group col-md-12">
    <%=text_field_tag :email, "", class: "form-control input-lg", placeholder: "Digite seu email"%>
  </div>
  <button type="submit" class="btn btn-primary">Pesquisar</button>
<%end%>
